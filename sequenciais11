#include <stdio.h>

/*5. Escreva um algoritmo para ler 10 números. Todos os números lidos com valor inferior a 40
devem ser somados. Escreva o valor final da soma efetuada.*/

int main(){
    int i;
    float numero, soma = 0;

    for(i = 0; i <= 9; i++){
        printf("Informe um numero\n"); 
        scanf("%f", &numero);

        if(numero < 40){
            soma = soma + numero;
        }
    }

    printf("SOMA: %2.f\n", soma);
}
