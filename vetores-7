#include <stdio.h>

/*1. Faça um programa com um vetor de números inteiros de tamanho 10 (com
qualquer número a sua escolha) e imprima-os no terminal dessa forma (utilizar
laço de repetição for):
Elemento 1 do vetor: 55
…
Elemento 10 do vetor: 20*/

int main()
{
    int numeros[10], i;

    for (i = 0; i < 9; i++)
    {
        printf("Informe o numero\n");
        scanf("%d", &numeros[i]);
    }

    for (i = 0; i < 9; i++)
    {
        printf("Elemento %d do vetor: %d\n", i + 1, numeros[i]);
    }
}
